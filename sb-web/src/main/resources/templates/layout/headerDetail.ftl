<section class="collection-head small geopattern" data-pattern-id="记一个 Base64 有关的 ">
    <div class="container">
        <div class="columns">
            <div class="column three-fourths">
                <div class="collection-title">
                    <h1 class="collection-header">${article.title}</h1>
                    <div class="collection-info"> <span class="meta-info"> <span class="octicon octicon-calendar"></span> ${article.createTime?string('yyyy/MM/dd')} </span>
                        <span class="meta-info">
                            <#list article.tags as item>
                                <span class="octicon octicon-file-directory"></span>
                                <a href="https://mazhuang.org/tag/${item.id}" title="${item.name}">${item.name}</a>
                            </#list>
                        </span>

                        <span class="meta-info">
                    </div>
                </div>
            </div>
            <div class="column one-fourth mobile-hidden">
                <div class="collection-title"> <img src="/me.jpg" style="height:72px;width:72px"/></div>
            </div>
        </div>
    </div>
</section>


<style>
    .collection-head{
        background-image:url("data:image/svg+xml;base64,");   }
</style>
